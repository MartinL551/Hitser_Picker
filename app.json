{
  "expo": {
    "name": "Music_Game_Selector",
    "slug": "Music_Game_Selector",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-localization"
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icons/recordV2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/recordV2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/recordV2.png",
        "backgroundColor": "#313131"
      },
      "package": "com.martin.music_game_selector",
      "versionCode": 1
    },
    "extra": {
      "debug": {
        "lang": "de"
      }
    }
  }
}
